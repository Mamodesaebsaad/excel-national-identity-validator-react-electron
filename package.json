{
  "name": "excel-national-identity-validator-react-electron",
  "main": "public/electron.js",
  "homepage": "./",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@material-ui/core": "^4.12.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.2",
    "concurrently": "^6.4.0",
    "electron-is-dev": "^2.0.0",
    "file-saver": "^2.0.5",
    "national-identity-validator": "^1.0.3",
    "react-spreadsheet": "^0.7.7",
    "scheduler": "^0.23.0",
    "xlsx": "^0.18.5",
    "react-hot-loader": "^4.13.0",
    "react-to-print": "^2.14.1",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:build-32": "npm run build && electron-builder build --win --ia32 -c.extraMetadata.main=build/electron.js"
    
  },
  "build": {
    "extends": null,
    "appId": "com.example.react-electron",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_moduese/**/*",
      "package.json"
    ],
    "asar": false,
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5"
  }
}
